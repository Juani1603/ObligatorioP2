@{
    ViewData["Title"] = "Login";
}

<h1>Login</h1>
<br />
<form method="post">
    <label>Correo</label>
    <input type="text" name="Correo" />
    <br />
    <label>Contraseña</label>
    <input type="password" name="Contrasena" />
    <input type="submit" value="Ingresar" />
</form>
<span class="alert-danger">@TempData["Mensaje"]</span>
@ViewBag.Mensaje